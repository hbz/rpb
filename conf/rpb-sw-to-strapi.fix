do put_macro("add")
  if exists("$[from]")
    set_array("_temp")
    move_field("$[from]", "_temp.$append.f$[to]")
    move_field("_temp.*", "f$[to][].$append")
  end
end

do put_macro("add_all")
  set_array("f$[to][]")
  call_macro("add", to: "$[to]", from: "f$[from]", type: "")
end

